{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
   "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"],
      "automerge": true
    }
  ],
 "vulnerabilityAlerts": {
    "enabled": true,
   "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "major", "lockFileMaintenance"],
      "matchPackagePatterns": [".*"],
      "groupName": "all dependencies",
      "groupSlug": "all"
    }
  ],
    "labels": ["RENOVATE"],
   "commitMessagePrefix": ["RENOVATE"],
    "automerge": true
  },
  "osvVulnerabilityAlerts": true,
  "ignoreTests": true,
  "platformAutomerge": true,
  "automergeType": "pr"
}
