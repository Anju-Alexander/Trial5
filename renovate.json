{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
   "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"],
      "automerge": true
    }
  ],
 "vulnerabilityAlerts": {
    "enabled": true,
  
  
    "labels": ["RENOVATE"],
   "commitMessagePrefix": ["RENOVATE"],
    "automerge": true
  },
   "packageRules": [
    {
       "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
   ]
  "osvVulnerabilityAlerts": true,
  "ignoreTests": true,
  "platformAutomerge": true,
  "automergeType": "pr"
}
